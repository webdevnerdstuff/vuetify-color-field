	"description": "Vuetify Color Field is a Vuetify VTextField Color Picker Component",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/vuetify-color-field.cjs.js",
	"module": "dist/vuetify-color-field.es.js",
	"types": "dist/types/index.d.ts",
