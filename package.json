		"deploy": "gh-pages -d docs",
		"prepublishOnly": "npm run build",
		"lint": "eslint src/**/*.{ts,vue} --max-warnings 20",
		"prepare": "husky install"
	},
	"lint-staged": {
		"src/**/*.{js,ts,vue}": [
			"npm run lint"
		]
